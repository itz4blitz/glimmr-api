{
  "name": "glimmr",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "dev:services": "docker-compose -f docker-compose.dev.yml up -d postgres redis minio",
    "dev:full": "docker-compose -f docker-compose.dev.yml up -d",
    "dev:stop": "docker-compose -f docker-compose.dev.yml down",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/unzipper": "^0.10.11",
    "jest": "^29.7.0",
    "prettier": "^3.5.3",
    "ts-jest": "^29.4.0",
    "turbo": "^2.5.4",
    "typescript": "5.8.2"
  },
  "packageManager": "pnpm@10.12.3",
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "@nestjs/throttler": "^6.4.0",
    "passport-headerapikey": "^1.2.2",
    "tar-stream": "^3.1.7",
    "unzipper": "^0.12.3",
    "xlsx": "^0.18.5"
  }
}
