{
  "name": "glimmr-api",
  "version": "0.0.1",
  "description": "Glimmr API - Hospital pricing data aggregation platform",
  "author": "Justin Scroggins",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:auth": "./test-auth.sh",
    "test:auth:unit": "jest src/auth --testPathIgnorePatterns=e2e",
    "test:auth:e2e": "jest test/auth.e2e-spec.ts test/rbac.e2e-spec.ts test/security.e2e-spec.ts --config ./test/jest-e2e.json",
    "check-types": "tsc --noEmit",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:seed": "tsx src/database/seeds/initial-data.ts",
    "db:seed:admin": "tsx src/database/seeds/admin-user.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.832.0",
    "@bull-board/api": "^6.10.1",
    "@bull-board/express": "^6.10.1",
    "@bull-board/nestjs": "^6.10.1",
    "@nestjs/bullmq": "^11.0.2",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^10.0.0",
    "@nestjs/passport": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/schedule": "^6.0.0",
    "@nestjs/swagger": "^7.0.0",
    "@nestjs/typeorm": "^10.0.0",
    "@types/adm-zip": "^0.5.7",
    "@types/papaparse": "^5.3.16",
    "adm-zip": "^0.5.16",
    "axios": "^1.5.0",
    "bcrypt": "^5.1.0",
    "bullmq": "^5.56.0",
    "cache-manager": "^5.0.0",
    "cache-manager-redis-store": "^3.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "drizzle-kit": "^0.31.1",
    "drizzle-orm": "^0.44.2",
    "drizzle-zod": "^0.5.1",
    "express": "^4.18.0",
    "helmet": "^7.0.0",
    "ioredis": "^5.6.1",
    "joi": "^17.9.0",
    "nest-winston": "^1.9.0",
    "nestjs-pino": "^4.4.0",
    "papaparse": "^5.5.3",
    "passport": "^0.6.0",
    "passport-headerapikey": "^1.2.2",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.11.0",
    "pino-http": "^10.5.0",
    "pino-pretty": "^13.0.0",
    "playwright": "^1.53.1",
    "postgres": "^3.4.7",
    "redis": "^4.6.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "uuid": "^9.0.0",
    "winston": "^3.10.0",
    "xlsx": "^0.18.5",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@repo/eslint-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@types/bcrypt": "^5.0.0",
    "@types/compression": "^1.8.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/passport-jwt": "^3.0.9",
    "@types/passport-local": "^1.0.35",
    "@types/pg": "^8.15.4",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.20.3",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
