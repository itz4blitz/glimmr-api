{
  "priceTypes": {
    "gross_charge": {
      "aliases": ["standard_charge", "list_price", "chargemaster_rate", "full_charge"],
      "definition": "The hospital's full established price before any discounts",
      "usage": "Rarely paid by anyone, used as starting point for negotiations"
    },
    "discounted_cash_price": {
      "aliases": ["cash_price", "self_pay_rate", "uninsured_rate"],
      "definition": "Price for patients paying directly without insurance",
      "usage": "Often lower than insurance negotiated rates"
    },
    "negotiated_rate": {
      "aliases": ["contracted_rate", "allowed_amount", "payer_specific_rate"],
      "definition": "Price negotiated between hospital and insurance company",
      "usage": "What insurance actually pays, varies by payer and plan"
    },
    "de-identified_minimum": {
      "aliases": ["min_rate", "lowest_negotiated"],
      "definition": "Lowest negotiated rate across all payers",
      "usage": "Required by CMS, helps price shopping"
    },
    "de-identified_maximum": {
      "aliases": ["max_rate", "highest_negotiated"],
      "definition": "Highest negotiated rate across all payers",
      "usage": "Shows price variation range"
    }
  },
